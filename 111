# -*- coding: UTF-8 -*-
import sys


if __name__ == "__main__":
   n, m = map(int, sys.stdin.readline().strip().split(" "))
   arr = map(int, sys.stdin.readline().strip().split(" "))
   sum = 0
   hight = max(arr)

 
   while sum != m and m!=0:
       hight -= 1
       for i in arr:
           if hight<i:
               sum+=1
   print hight
