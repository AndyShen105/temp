#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include<string>
#include <fstream>
#include<sstream>
using namespace std;
#define CIN myin



bool isval(vector<int>&aC , int C )
{
	int n = aC.size();
	vector<int>f((++C), -10000);
	f[0] = 0;
	for (int i = 0; i <= n - 1; i++)
	{
		for (int j = C - 1; j >= aC[i]; j--)
		{
			if (i == 0)
			{
				if (j == aC[i])f[j] = 1;
			}
			else
			{
				f[j] = max(f[j - aC[i]] + 1, f[j]);
				if (f[j] < 0)f[j] = -10000;
			}
		}

	}
	return (*f.rbegin()>0);
}
int main()
{
	int n;
	cin >> n;
	vector<int>a(n);
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	int C;
	cin >> C;
	cout << isval(a,C);
	return 0;
}
 
