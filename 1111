import sys


def f(list):

    re = 0
    for i in range(len(list)):
        for j in range(i):
            if list[j] > list[i]:
                re += 1
    return re


if __name__ == "__main__":
    n = int(sys.stdin.readline().strip())
    numbers = []
    for i in range(n):
        a = int(sys.stdin.readline().strip())
        numbers.append(a)
    print(f(numbers))
